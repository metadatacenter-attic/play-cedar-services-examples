# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET         /                          controllers.Application.index()
GET         /telements/:id             controllers.CrudExampleController.findTElementById(id: String)
GET         /telements/                controllers.CrudExampleController.findAllTElements()
GET         /telements                 controllers.CrudExampleController.findAllTElements()
POST        /telements                 controllers.CrudExampleController.createTElement()
PUT         /telements/:id             controllers.CrudExampleController.updateTElement(id: String)
DELETE      /telements/:id             controllers.CrudExampleController.deleteTElement(id: String)

## Map static resources from the /public folder to the /assets URL path
#GET         /assets/*file            controllers.Assets.at(path="/public", file)